@startchen

entity Beneficiary {
    beneficiaryID <<key>>
    name
    email
    phones {
        local
        cell
    }
    residence {
        state
        municipality
        parish
    }
    sexo
    '(M,F)'
    birthdate
    nacionalidad Ven Ext
    maritalStatus
    concubinage
    education
    '(0-14)'
    educationDate
    dwellingType
    bedrooms
    bathrooms
    floorMaterial
    '(Tierra, Cemento, Ceramica, Granito/Parquet/Marmol)'
    wallMaterial
    '(Carton/Palma/Desechos, Bahareque, Bloque sin frizar, Bloque frizado)'
    roofMaterial
    '(Madera/Carton/Palma, Zinc/Acerolit, Platabanda/Tejas)'
    drinkingWaterService
    '(Dentro de la vivienda, Fuera de la vivienda, No tiene servicio)'
    sewageService
    'Poceta a cloaca/Poze septico, Poceta sin conexion (tubo), Excusado de hoyo o letrina, No tiene)'
    pickUpTrashService
    '(Llega vivienda, No llega vivienda/Container, No tiene)'
    domesticArtifacts {
        
    }
    isWorking
}
entity BeneficiaryWork {
    workCondition
    '(Patrono, Empleado, Obrero, Cuanta propia)'
}
entity BeneficiaryNoWork{
    wantWork
}
entity BeneficiaryWantWork{
}
entity BeneficiaryNoWantWork{
    activityCondition
    '(Ama de casa, Estudiante, Pensionado/Jubilado, Otra)'
}

Beneficiary =>= U {BeneficiaryWork, BeneficiaryNoWork}
BeneficiaryNoWork =>= U {BeneficiaryWantWork, BeneficiaryNoWantWork}

entity Caso {
}
entity Seguimiento {
}
entity Profesor {
}
entity Alumno {
}


@endchen
