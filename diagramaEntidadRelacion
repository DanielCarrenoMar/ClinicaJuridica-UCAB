@startchen

entity Beneficiary {
    beneficiaryID <<key>>
    name
    email
    phones {
        local
        cell
    }
    residence {
        state
        municipality
        parish
    }
    sexo
    'M F'
    birthdate
    nacionalidad Ven Ext
    maritalStatus
    concubinage
    education
    '0 14'
    educationDate
    dwellingType
    '(Quinta/Casa Urb, Apartamento, Bloque, Casa de Barrio, )'
    isWorking
}
entity BeneficiaryWork {
    workCondition
    '(Patrono, Empleado, Obrero, Cuanta propia)'
}
entity BeneficiaryNoWork{
    wantWork
}
entity BeneficiaryWantWork{
}
entity BeneficiaryNoWantWork{
    activityCondition
    '(Ama de casa, Estudiante, Pensionado/Jubilado, Otra)'
}

Beneficiary =>= U {BeneficiaryWork, BeneficiaryNoWork}
BeneficiaryNoWork =>= U {BeneficiaryWantWork, BeneficiaryNoWantWork}

entity Caso {
}
entity Seguimiento {
}
entity Profesor {
}
entity Alumno {
}


@endchen
