@startchen

entity Beneficiary {
    beneficiaryID <<key>>
    name
    email
    phones {
        local
        cell
    }
    residence {
        state
        municipality
        parish
    }
    sexo
    '(M,F)'
    birthdate
    nacionalidad Ven Ext
    maritalStatus
    concubinage
    education
    '(0-14)'
    educationDate
    dwellingType
    bedrooms
    bathrooms
    floorMaterial
    '(Tierra, Cemento, Ceramica, Granito/Parquet/Marmol)'
    wallMaterial
    '(Carton/Palma/Desechos, Bahareque, Bloque sin frizar, Bloque frizado)'
    roofMaterial
    '(Madera/Carton/Palma, Zinc/Acerolit, Platabanda/Tejas)'
    drinkingWaterService
    '(Dentro de la vivienda, Fuera de la vivienda, No tiene servicio)'
    sewageService
    'Poceta a cloaca/Poze septico, Poceta sin conexion (tubo), Excusado de hoyo o letrina, No tiene)'
    pickUpTrashService
    '(Llega vivienda, No llega vivienda/Container, No tiene)'
    domesticArtifacts {
        refrigerator
        washingMachine
        computer
        satelliteCable
        internet
        car
        motorcycle
    }
    isWorking
    householdSize
    isHeadOfHousehold
    headOfHouseholdEducation
    '(Sin Nivel, Primaria, Basica, Media Diversificada, Tecnico Medio, Tecnico Superior, Universitaria)'
    headOfHouseholdEducationPeriod
    '(AÃ±os, Semestres, Trimestres)'
    householdMembersWorking
    householdMembersNotWorking
    children7to12
    childrenStudying
    monthlyIncome
}
entity BeneficiaryWork {
    workCondition
    '(Patrono, Empleado, Obrero, Cuanta propia)'
}
entity BeneficiaryNoWork{
    wantWork
}
entity BeneficiaryWantWork{
}
entity BeneficiaryNoWantWork{
    activityCondition
    '(Ama de casa, Estudiante, Pensionado/Jubilado, Otra)'
}
Beneficiary =>= U {BeneficiaryWork, BeneficiaryNoWork}
BeneficiaryNoWork =>= U {BeneficiaryWantWork, BeneficiaryNoWantWork}

entity Case {
    caseID <<key>>
    description
    status
    '(Abierto, Cerrado, En espera)'
    startDate
    endDate
}
entity Profesor {
    profesorID <<key>>
    name
    email
}
entity Alumno {
    alumnoID <<key>>
    name
    email
}
entity Seguimiento {
    seguimientoID <<key>>
    date
    description
}
entity Documento {
    documentoID <<key>>
    fileName
    fileType
    uploadDate
}

relationship Has {
}
relationship Supervises {
}
relationship WorksOn {
}
relationship Contains {
}
relationship Includes {
}

Beneficiary -1- Has
Has -N- Case

Profesor -1- Supervises
Supervises -N- Case

Alumno -N- WorksOn
WorksOn -1- Case

Case -1- Contains
Contains -N- Seguimiento

Case -1- Includes
Includes -N- Documento

@endchen
